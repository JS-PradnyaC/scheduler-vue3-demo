{"ast":null,"code":"import \"core-js/modules/es.array.concat.js\";\nimport Scheduler from './components/Scheduler.vue';\nexport default {\n  name: 'app',\n  components: {\n    Scheduler: Scheduler\n  },\n  data: function data() {\n    return {\n      events: [{\n        id: 1,\n        start_date: \"2020-01-20 6:00\",\n        end_date: \"2020-01-20 15:00\",\n        text: \"Event 1\"\n      }, {\n        id: 2,\n        start_date: \"2020-01-23 6:00\",\n        end_date: \"2020-01-23 20:00\",\n        text: \"Event 2\"\n      }],\n      messages: []\n    };\n  },\n  methods: {\n    addMessage: function addMessage(message) {\n      this.messages.unshift(message);\n\n      if (this.messages.length > 40) {\n        this.messages.pop();\n      }\n    },\n    logEventUpdate: function logEventUpdate(id, mode, ev) {\n      var text = ev && ev.text ? \" (\".concat(ev.text, \")\") : '';\n      var message = \"Event \".concat(mode, \": \").concat(id, \" \").concat(text);\n      this.addMessage(message);\n    }\n  }\n};","map":{"version":3,"mappings":";AAYA,OAAOA,SAAP,MAAsB,4BAAtB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IAACF,SAAS,EAATA;EAAD,CAFC;EAGbG,IAHa,kBAGL;IACN,OAAO;MACLC,MAAM,EAAE,CACN;QAAEC,EAAE,EAAC,CAAL;QAAQC,UAAU,EAAC,iBAAnB;QAAsCC,QAAQ,EAAC,kBAA/C;QAAmEC,IAAI,EAAC;MAAxE,CADM,EAEN;QAAEH,EAAE,EAAC,CAAL;QAAQC,UAAU,EAAC,iBAAnB;QAAsCC,QAAQ,EAAC,kBAA/C;QAAmEC,IAAI,EAAC;MAAxE,CAFM,CADH;MAKLC,QAAQ,EAAE;IALL,CAAP;EAOD,CAXY;EAYbC,OAAO,EAAE;IACPC,UADO,sBACKC,OADL,EACc;MACnB,KAAKH,QAAL,CAAcI,OAAd,CAAsBD,OAAtB;;MACA,IAAI,KAAKH,QAAL,CAAcK,MAAd,GAAuB,EAA3B,EAA+B;QAC7B,KAAKL,QAAL,CAAcM,GAAd;MACF;IACD,CANM;IAQPC,cARO,0BAQSX,EART,EAQaY,IARb,EAQmBC,EARnB,EAQuB;MAC5B,IAAIV,IAAG,GAAKU,EAAC,IAAKA,EAAE,CAACV,IAAT,eAAqBU,EAAE,CAACV,IAAxB,SAAiC,EAA7C;MACA,IAAII,OAAM,mBAAaK,IAAb,eAAsBZ,EAAtB,cAA4BG,IAA5B,CAAV;MACA,KAAKG,UAAL,CAAgBC,OAAhB;IACD;EAZM;AAZI,CAAf","names":["Scheduler","name","components","data","events","id","start_date","end_date","text","messages","methods","addMessage","message","unshift","length","pop","logEventUpdate","mode","ev"],"sourceRoot":"","sources":["C:\\Users\\Vaibhav\\vue-scheduler-demo\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <scheduler class=\"left-container\" :events=\"events\" @event-updated=\"logEventUpdate\"></scheduler>\r\n    <div class=\"right-container\">\r\n      <ul class=\"scheduler-messages\">\r\n        <li class=\"scheduler-message\" v-for=\"message in messages\">{{message}}</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nimport Scheduler from './components/Scheduler.vue';\r\n \r\nexport default {\r\n  name: 'app',\r\n  components: {Scheduler},\r\n  data () {\r\n    return {\r\n      events: [\r\n        { id:1, start_date:\"2020-01-20 6:00\", end_date:\"2020-01-20 15:00\", text:\"Event 1\"},\r\n        { id:2, start_date:\"2020-01-23 6:00\", end_date:\"2020-01-23 20:00\", text:\"Event 2\"}\r\n      ],\r\n      messages: []\r\n    }\r\n  },\r\n  methods: {\r\n    addMessage (message) {\r\n      this.messages.unshift(message)\r\n      if (this.messages.length > 40) {\r\n        this.messages.pop()\r\n      }\r\n    },\r\n\r\n    logEventUpdate (id, mode, ev) {\r\n      let text = (ev && ev.text ? ` (${ev.text})`: '')\r\n      let message = `Event ${mode}: ${id} ${text}`\r\n      this.addMessage(message)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  html, body {\r\n    height: 100%;\r\n    margin: 0;\r\n    padding: 0;\r\n  }\r\n\r\n  .container {\r\n    height: 100%;\r\n    width: 100%;\r\n  }\r\n\r\n  .left-container {\r\n    overflow: hidden;\r\n    position: relative;\r\n    height: 100vh;\r\n    display: inline-block;\r\n    width: 82vw;\r\n  }\r\n\r\n  .right-container {\r\n    border-right: 1px solid #cecece;\r\n    float: right;\r\n    height: 100%;\r\n    width: 340px;\r\n    box-shadow: 0 0 5px 2px #aaa;\r\n    position: relative;\r\n    z-index:2;\r\n  }\r\n\r\n.scheduler-messages {\r\n    list-style-type: no